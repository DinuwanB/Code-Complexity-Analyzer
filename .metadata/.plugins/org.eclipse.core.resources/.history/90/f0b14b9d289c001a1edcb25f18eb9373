        int classDefObjCount = 0;
        int classCount = 0;
        int methodCount = 0;
        String varData = "";
        int prist = 0;
        int inForLoopVariablesCount = 0;
        int calc = 0;

        ArrayList<String> identifiresofforloop = new ArrayList<String>();
        ArrayList<String> insideprintSt = new ArrayList<String>();
        
            
        
		return 0;
	}
}
       

